@{ 
    var name = "";
    if(Request["name"] != null)
    {
        name = Request["name"];
    } else
    {
        name = "Robert Paulson";
    }
}

<!DOCTYPE html>
<html>
<head>
    <title></title>
</head>
<body>
    <ul>
        @for (int i = 0; i < 10; i++)
        {
            <li>His name is @name</li>
        }
    </ul>

</body>
</html>
